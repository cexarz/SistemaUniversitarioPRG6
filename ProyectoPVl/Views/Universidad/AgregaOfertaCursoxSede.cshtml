@model ProyectoPVl.Models.RetornaCuatrimestreXSedeID_Result


<h2>Agregar una oferta de cursos por sede</h2>

<form id="frmAgregaOfertaCursoxSede" method="post">

	<input type="hidden" id="id_CuatrimestreXSede" value="@Model.id_CuatrimestreXSede" />

	<div align="center">
		<label for="Ano">Año:</label>
		<input type="text" name="Ano" id="Ano" class="form-control" value="@Model.Ano" readonly />
	</div>

	<br />

	<div align="center">
		<label>Sede:</label>
		<input type="text" name="Sede" id="Sede" class="form-control" value="@Model.SedeUniversitaria" readonly />
	</div>

	<br />

	<div align="center">
		<label>Cuatrimestre:</label>
		<input type="text" name="Cuatrimestre" id="Cuatrimestre" class="form-control" value="@Model.PeriodoLectivo" readonly />
	</div>

	<br />

	<div align="center">
		<label>Curso:</label>
		<select name="Curso" id="Curso" class="form-control"></select>
	</div>

	<br />

	<div align="center">
		<label>Cupos disponibles:</label>
		<input type="text" name="Cupos" id="Cupos" class="form-control" />
	</div>

	<br />

	<div align="center">
		<input type="button" value="Ingresar" id="btnIngresar" class="btn-primary" />
	</div>

</form>

@section scripts{
	<script src="~/Scripts/Universidad/AgregaOfertaCursoxSede.js"></script>
}

